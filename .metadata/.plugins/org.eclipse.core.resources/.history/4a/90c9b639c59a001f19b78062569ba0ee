package pkg1;

public class M1 {

	public static void main(String[] args) {
		for(int i=0;i<=5;i++) {
			System.out.print(i*2+ " ");
		}
		System.out.println("");
		for(int i=0;i<=5;i++) {
			 System.out.print(i*2+1+" ");
		}
		System.out.println("");
		for(int i=0;i<=5;i++) {
			System.out.print(i*3+" ");
		}
		System.out.println("");
		for(int i=0;i<=5;i++) {
			System.out.print(i*4+" ");
		}
		System.out.println("");
		for(int i=0;i<=5;i++){
			System.out.print(i*5+" ");
			
		}
	}
}
